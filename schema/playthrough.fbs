namespace DS3C;

table Location{
  name: string; // markdown
  note: string; // null
}

table Task{
  data_id: string;
  tags: [string];
  text: string;	// markdown
}

table Playthrough{
  location: Location;
  tasks: [Task];
}

table PlaythroughRoot{
  items: [Playthrough];
}

root_type PlaythroughRoot;
